<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lshop.manage.weixin.pojo">
    <class name="WxGzhConfig" table="wx_gzh_config">

        <id name="id" column="id" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
		
		
		
		<property 
			name="wxhName" 
			column="wxh_name" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="32"/>
		
		<property 
			name="preUrl" 
			column="pre_url" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="255"/>
		
		<property 
			name="token" 
			column="token" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="32"/>
		
		<property 
			name="encodingAesKey" 
			column="encoding_aes_key" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="43"/>
		
		<property 
			name="appId" 
			column="app_id" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="32"/>
		
		<property 
			name="appSecret" 
			column="app_secret" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="128"/>
		
		<property 
			name="accessToken" 
			column="access_token" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="512"/>
		
		<property 
			name="accessTokenExpires" 
			column="access_token_expires" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="10"/>
		
		<property 
			name="accessTokenTime" 
			column="access_token_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="code" 
			column="code" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="createTime" 
			column="create_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="modifyTime" 
			column="modify_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="modifyUserId" 
			column="modify_user_id" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="10"/>
		
		<property 
			name="modifyUserName" 
			column="modify_user_name" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
			<property 
			name="storeId" 
			column="store_id" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		

	

    </class>
</hibernate-mapping>

