<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lshop.manage.weixin.pojo">
    <class name="WxMenu" table="wx_menu">

        <id name="id" column="id" type="java.lang.Integer">
            <generator class="identity"/>
        </id>
		
		
		
		<property 
			name="name" 
			column="name" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="16"/>
		
		<property 
			name="menuType" 
			column="menu_type" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="16"/>
		
		<property 
			name="menuKey" 
			column="menu_key" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="64"/>
		
		<property 
			name="menuUrl" 
			column="menu_url" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="256"/>
		
		<property 
			name="materialType" 
			column="material_type" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="10"/>
		
		<property 
			name="materialId" 
			column="material_id" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="10"/>
		
		<property 
			name="menuParent" 
			column="menu_parent" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="true" 
			unique="false" 
			length="10"/>
		
		<property 
			name="orderValue" 
			column="order_value" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="10"/>
		
		<property 
			name="wxhConfigId" 
			column="wxh_config_id" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="true" 
			unique="false" 
			length="10"/>
		
		<property 
			name="createTime" 
			column="create_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="true" 
			unique="false" 
			length="0"/>
		
		

	

    </class>
</hibernate-mapping>

