<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.gv.appstore.pojo">
    <class name="LvDeveloperApp" table="lv_developer_app">

        <id name="id" column="id" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
		
		
		
		<property 
			name="name" 
			column="name" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="32"/>
		
		<property 
			name="url" 
			column="url" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="128"/>
		
		<property 
			name="appVersion" 
			column="app_version" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="16"/>
		
		<property 
			name="lang" 
			column="lang" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="16"/>
		
		<property 
			name="appType" 
			column="app_type" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="16"/>
		
		<property 
			name="keyWord" 
			column="key_word" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="appDesc" 
			column="app_desc" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="1500"/>
		
		<property 
			name="proofUrl" 
			column="proof_url" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="128"/>
		
		<property 
			name="demoImgsUrl" 
			column="demo_imgs_url" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="1000"/>
		
		<property 
			name="isCharge" 
			column="is_charge" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="isAd" 
			column="is_ad" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="createTime" 
			column="create_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="updateTime" 
			column="update_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="reviewTime" 
			column="review_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="email" 
			column="email" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="60"/>
		
		<property 
			name="reviewStatus" 
			column="a_status" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="10"/>

		<property 
			name="reason" 
			column="reason" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="128"/>
			
		<property 
			name="code" 
			column="code" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
			
		<property 
			name="bizline" 
			column="bizline" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="4"/>
			
		<property 
			name="storeFlag" 
			column="store_flag" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="16"/>
			
		<property 
			name="isAgree" 
			column="is_agree" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="0"/>

    </class>
</hibernate-mapping>

