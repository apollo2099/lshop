<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.gv.appstore.pojo">
    <class name="LvDeveloper" table="lv_developer">

        <id name="id" column="id" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
		
		
		
		<property 
			name="email" 
			column="email" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="60"/>
		
		<property 
			name="contactName" 
			column="contact_name" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="company" 
			column="company" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="128"/>
		
		<property 
			name="dstatus" 
			column="d_status" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="true" 
			unique="false" 
			length="10"/>
		
		<property 
			name="createTime" 
			column="create_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="true" 
			unique="false" 
			length="0"/>
		
		<property 
			name="reviewTime" 
			column="review_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="updateTime" 
			column="update_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="dtype" 
			column="d_type" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="idNum" 
			column="id_num" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="idUrl" 
			column="id_url" 
			update="true"
            insert="true"
            type="java.sql.Blob"
			not-null="false" 
			unique="false" 
			length="128"/>
		
		<property 
			name="tel" 
			column="tel" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="address" 
			column="address" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="128"/>
		
		<property 
			name="agreeImgsUrl" 
			column="agree_imgs_url" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="128"/>

		<property 
			name="reason" 
			column="reason" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="128"/>

		<property 
			name="code" 
			column="code" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
			
		<property 
			name="bizline" 
			column="bizline" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="4"/>
			
		<property 
			name="storeFlag" 
			column="store_flag" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="16"/>
		
		<property 
			name="isAgree" 
			column="is_agree" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="0"/>
    </class>
</hibernate-mapping>

