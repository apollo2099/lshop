<struts>

	<package name="freeManage-default" namespace="/web" extends="gv-default">

		<action name="free" class="FreeManageAction">
			<result name="toFree">/web/${flag}/free/free_trial.jsp</result>
			<result name="toFreeAction" type="redirect">/web/free!toFree.action?mark=${mark}</result>
		</action>

	</package>

</struts>
