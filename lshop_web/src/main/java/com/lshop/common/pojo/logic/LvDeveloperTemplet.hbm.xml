<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lshop.common.pojo.logic">
    <class name="LvDeveloperTemplet" table="lv_developer_templet">

        <id name="id" column="id" type="java.lang.Integer">
            <generator class="identity"/>
        </id>
		
		
		
		<property 
			name="name" 
			column="name" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="32"/>
		
		<property 
			name="title" 
			column="title" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="32"/>
		
		<property 
			name="content" 
			column="content" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="1500"/>
		
		<property 
			name="mallSign" 
			column="mall_sign" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="64"/>
		
		<property 
			name="ttype" 
			column="t_type" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="true" 
			unique="false" 
			length="10"/>
		
		<property 
			name="createTime" 
			column="create_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="true" 
			unique="false" 
			length="0"/>
		
		<property 
			name="updateTime" 
			column="update_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="updateUser" 
			column="update_user" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="status" 
			column="t_status" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="true" 
			unique="false" 
			length="10"/>

	

    </class>
</hibernate-mapping>

