<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lshop.common.pojo.logic">
    <class name="LvMappCardsUser" table="lv_mapp_cards_user">

        <id name="id" column="id" type="java.lang.Integer">
            <generator class="identity" />
        </id>
		
		
		
		<property 
			name="recipient" 
			column="recipient" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="16"/>
		
		<property 
			name="cdesc" 
			column="c_desc" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="256"/>
		
		<property 
			name="code" 
			column="code" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="32"/>
		
		<property 
			name="sender" 
			column="sender" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="16"/>
		
		<property 
			name="tempCode" 
			column="temp_code" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="32"/>
		
		<property 
			name="fallType" 
			column="fall_type" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="10"/>
		
		<property 
			name="textType" 
			column="text_type" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="10"/>
		
		<property 
			name="musicType" 
			column="music_type" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="10"/>
		
		<property 
			name="createTime" 
			column="create_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="imgUrl" 
			column="img_url" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="128"/>
		
		

	

    </class>
</hibernate-mapping>

