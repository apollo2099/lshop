<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lshop.web.weixin.common.pojo">
    <class name="WxUser" table="wx_user">

        <id name="id" column="id" type="java.lang.Integer">
            <generator class="identity"/>
        </id>
		
		
		
		<property 
			name="openid" 
			column="openid" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="true" 
			length="64"/>
		
		<property 
			name="nickname" 
			column="nickname" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="64"/>
		
		<property 
			name="email" 
			column="email" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="128"/>
		
		<property 
			name="bindTime" 
			column="bind_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="wxhConfigId" 
			column="wxh_config_id" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="true" 
			unique="false" 
			length="10"/>
		
		<property 
			name="createTime" 
			column="create_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="true" 
			unique="false" 
			length="0"/>
		
		<property 
			name="modifyTime" 
			column="modify_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="obtainAmount" 
			column="obtain_amount" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="true" 
			unique="false" 
			length="10"/>
		<property 
			name="lastObtainTime" 
			column="last_obtain_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
    </class>
</hibernate-mapping>

