<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lshop.web.weixin.common.pojo">
    <class name="WxNewsMaterialItem" table="wx_news_material_item">

        <id name="id" column="id" type="java.lang.Integer">
            <generator class="identity"/>
        </id>
		
		
		
		<property 
			name="newsType" 
			column="news_type" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="true" 
			unique="false" 
			length="10"/>
		
		<property 
			name="newsId" 
			column="news_id" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="true" 
			unique="false" 
			length="10"/>
		
		<property 
			name="title" 
			column="title" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="64"/>
		
		<property 
			name="description" 
			column="description" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="512"/>
		
		<property 
			name="url" 
			column="url" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="256"/>
		
		<property 
			name="picurl" 
			column="picurl" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="256"/>
		
		<property 
			name="wxhConfigId" 
			column="wxh_config_id" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="true" 
			unique="false" 
			length="10"/>
		
		<property 
			name="createTime" 
			column="create_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="true" 
			unique="false" 
			length="0"/>
		
		<property 
			name="modifyTime" 
			column="modify_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		

	

    </class>
</hibernate-mapping>

